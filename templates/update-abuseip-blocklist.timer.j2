{{ ansible_managed | comment }}

[Unit]
Description=Run update-abuseip-blocklist daily at 5 minutes past midnight
Requires=update-abuseip-blocklist.service
OnFailure=failmail@%N.service

[Timer]
Unit=update-abuseip-blocklist.service
OnCalendar=*-*-* 00:05:00
Persistent=true

[Install]
WantedBy=timers.target
